        "label": "Graph Varients",
        "pieChartPolar": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Pie (Polar)",
          "value": "pieChart"
        },
        "pieChartNonPolar": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Pie (Non Polar)",
          "value": "pieChartNonPolar"
        },
        "barChartVertical": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Bar (Vertical)",
          "value": "barChartVertical"
        },
        "barChartHorizontal": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Bar (Horizontal)",
          "value": "barChartHorizontal"
        },
        "lineChartWithlabel": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Line Chart (With Label)",
          "value": "lineChartWithlabel"
        },
        "lineChartWithoutlabel": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Line Chart (Without Label)",
          "value": "lineChartWithoutlabel",
          "selected": "true"
