{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "general": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "title": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "title",
        "description": "Title of the ticker, this is used to sync into Flow when its enabled.",
        "label": "Title",
        "defaultValue": "Demo"
      }
    },
    "styleTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/style"
    },
    
    "gridTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/grid"
    },
    "alignmentTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/alignment"
    },
    "writeTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Write",
      "writePayloadType": {
        "sling:resourceType": "wcm/dialogs/components/select",
        "name": "writePayloadType",
        "label": "Payload Type",
        "form": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "FORM",
          "value": "application/x-www-form-urlencoded"
        },
        "json": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "JSON",
          "value": "application/json"
        }
      },
      "writeMethod": {
        "sling:resourceType": "wcm/dialogs/components/select",
        "name": "writeMethod",
        "label": "Method",
        "form": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "POST Request",
          "value": "POST"
        },
        "json": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "PUT Request",
          "value": "PUT"
        }
      },
      "writeUrl": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "writeUrl",
        "label": "Endpoint URL"
      }
    },
    "readTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Read",
      "readMethod": {
        "sling:resourceType": "wcm/dialogs/components/select",
        "name": "readMethod",
        "label": "Method",
        "form": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "GET Request",
          "value": "GET"
        },
        "json": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "POST Request",
          "value": "POST"
        }
      },
      "readUrl": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "readUrl",
        "label": "Endpoint URL"
      }
    },
    "flowTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/flow/flowcontainer/dialog/tabs/flowTab"
    }
  }
}
