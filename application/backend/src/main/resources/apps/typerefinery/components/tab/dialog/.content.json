{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "configTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "tabList": {
        "name": "tabList",
        "label": "Tab Details",
        "sling:resourceType": "wcm/dialogs/components/multifield",
        "title": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "title",
          "label": "Title ",
          "description": "These will be added as  tab title"
        },
        "tabId": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "tabId",
          "label": "Tab Id",
          "description": "These will be added as tab Id"
        },
        "height": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "height",
          "label": "Tab height",
          "description": "These will be added as tab height"
        },
        "url": {
          "sling:resourceType": "wcm/dialogs/components/textfield",
          "name": "url",
          "label": "Iframe Url",
          "description": "These will be added as iframe Url"
        }
      }
    },
    "dataTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Data",
      "dataSource": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "dataSource",
        "description": "Please provide url to data source.",
        "label": "Data Source"
      }
    },
    "websocketTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/websocket"
    },
    "flowTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/flow/flowcontainer/dialog/tabs/flowTab"
    },
    "gridTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/grid"
    },
    "styleTab": {
      "sling:resourceType": "wcm/dialogs/components/include",
      "path": "/apps/typerefinery/components/dialog/tabs/style"
    }
  }
}