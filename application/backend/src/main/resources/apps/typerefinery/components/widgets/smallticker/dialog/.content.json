{
  "sling:resourceType": "wcm/dialogs/dialog",
  "tabs": {
    "sling:resourceType": "wcm/dialogs/components/tabs",
    "generalTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "General",
      "fieldTitle": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "title",
        "label": "Title"
      },
      "fieldValue": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "value",
        "default": "556578",
        "label": "Value"
      }
    },
    "styleTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "Style",
      "backGroundVariant": {
        "sling:resourceType": "wcm/dialogs/components/select",
        "name": "backGroundClass",
        "label": "Background Color",
        "primary": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Primary",
          "value": "primary",
          "selected": true
        },
        "secondary": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Secondary",
          "value": "secondary"
        },
        "indigo": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Indigo",
          "value": "indigo"
        },
        "purple": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Purple",
          "value": "purple"
        },
        "light-brown": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Light Brown",
          "value": "light-brown"
        },
        "light-green": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Light Green",
          "value": "light-green"
        },
        "green": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Green",
          "value": "green"
        },
        "dark-blue": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Dark Blue",
          "value": "dark-blue"
        },
        "brown": {
          "sling:resourceType": "wcm/dialogs/components/select/selectitem",
          "label": "Brown",
          "value": "brown"
        }
      }
    },
    "websocketTab": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "WebSocket Data Source",
      "websocketHost": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "websocketHost",
        "label": "Websocket Host"
      },
      "websocketTopic": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "websocketTopic",
        "label": "Websocket Topic"
      }
    },
    "dataSource": {
      "sling:resourceType": "wcm/dialogs/components/tab",
      "label": "JSON Data Source",
      "dataSource": {
        "sling:resourceType": "wcm/dialogs/components/textfield",
        "name": "dataSource",
        "description": "Please provide url to data source for this ticker.",
        "label": "Data Source"
      }
    }
  }
}
