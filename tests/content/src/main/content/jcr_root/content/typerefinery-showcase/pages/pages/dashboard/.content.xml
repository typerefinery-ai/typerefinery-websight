<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:ws="http://ds.pl/websight"
  xmlns:sling="http://sling.apache.org/jcr/sling/1.0" jcr:primaryType="ws:Page">
  <jcr:content jcr:primaryType="ws:PageContent" jcr:title="Dashboard" sling:resourceType="typerefinery/components/structure/page" ws:template="/apps/typerefinery/templates/page">
  <rootcontainer jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/fixedrootcontainer">
    <header jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/header" cancelInheritParent="true" classes="bg-secondary mb-3">
      <reference jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/reference"  path="/content/typerefinery-showcase/pages/content/simpleheader/jcr:content/container"></reference>
    </header>
      <container jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container">
        <container1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" flexEnabled="true" horizontalAlignment="around" classes="mt-3">
          <container1_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="2">
            <ticker jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/widgets/ticker"  ></ticker>
          </container1_1>
          <container1_2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="2">
            <ticker jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/widgets/ticker" templateSelected="1" tittle="Number of Inactive Users" value="15.7k" indicatorType="error"></ticker></container1_2>
          <container1_3 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="2">
            <ticker jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/widgets/ticker" templateSelected="1" tittle="Number of Users Subscribed" value="26.7k"></ticker></container1_3>
            <container1_4 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="2">
              <ticker jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/widgets/ticker" tittle="Total Count" value="67.9k" tickerIcon="glyphicon glyphicon-baby-formula"></ticker>
            </container1_4>
        
         </container1>
          <container2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" classes="mt-3">
            <chart jcr:primaryType= "nt:unstructured" sling:resourceType= "/apps/typerefinery/components/widgets/chart" variant="lineChart" />    
          
          </container2>
          <container3 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" flexEnabled="true" classes="mt-3">
            <container3_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="5">
              <chart jcr:primaryType= "nt:unstructured" sling:resourceType= "/apps/typerefinery/components/widgets/chart" variant="barChart" />
            </container3_1>
            <container3_2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" lgColSize="5">
              <chart jcr:primaryType= "nt:unstructured" sling:resourceType= "/apps/typerefinery/components/widgets/chart" variant="pieChart" /></container3_2>
          </container3>
          <container4 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" flexEnabled="true" classes="mt-3">
            <container4_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container"  lgColSize="5">
          
            </container4_1>
            <container4_2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container"  lgColSize="5">
          
            </container4_2>
          </container4>
      </container>
    </rootcontainer>
  </jcr:content>
</jcr:root>