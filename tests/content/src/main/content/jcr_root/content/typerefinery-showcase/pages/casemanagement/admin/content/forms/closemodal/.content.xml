<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:ws="http://ds.pl/websight"
  xmlns:sling="http://sling.apache.org/jcr/sling/1.0" jcr:primaryType="ws:Page">
  <jcr:content jcr:primaryType="ws:PageContent" jcr:title="Close Modal" sling:resourceType="typerefinery/components/structure/page" ws:template="/apps/typerefinery/templates/pagewithsidebar">
    <rootcontainer jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/fixedrootcontainer">
      <container jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container" flexEnabled="true" >
        <form1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/form" marginTop="one" marginEnabled="true">
          <title jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/title" title="Close Information" textAlignment="left" headingSize="h5"/>
          <input_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/input">
            <label jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/label" label="Case ID"/>
            <field jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/input" inputType="text" placeholder="Eg. case_B9SNEE3KG"/>
          </input_1>
          <select_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/select">
            <label jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/label" label="Reason" />
            <field jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/select" placeholder="Select the reason" />
          </select_1>
          <select_2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/select">
            <label jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/label" label="Status" />
            <field jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/fields/select" placeholder="Select the status" />
          </select_2>
        </form1>
      </container>
    </rootcontainer>
  </jcr:content>
</jcr:root>