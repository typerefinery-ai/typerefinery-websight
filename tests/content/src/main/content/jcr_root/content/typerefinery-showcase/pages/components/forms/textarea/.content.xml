<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
  xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
  xmlns:ws="http://ds.pl/websight"
  xmlns:sling="http://sling.apache.org/jcr/sling/1.0" jcr:primaryType="ws:Page">
  <jcr:content jcr:primaryType="ws:PageContent" jcr:title="TextArea Field's" sling:resourceType="typerefinery/components/structure/page" ws:template="/apps/typerefinery/templates/page">
    <rootcontainer jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/fixedrootcontainer">
      <header jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/header"/>
      <maincontainer jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/maincontainer">
        <container1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container">
          <container1_1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/layout/container">
            <title jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/title" title="Typerefinery Textarea's" headingSize="XL" />
          </container1_1>
          <textarea1 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea">
            <text jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/text" text="Enter User Temporary Address" />
            <textarea jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea/variant" placeholder="Eg. Delhi, India "/>
          </textarea1>
          <textarea2 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea">
            <text jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/text" text="Enter User Permanent Address" />
            <textarea jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea/variant" placeholder="Eg. UP, India "/>
          </textarea2>
          <textarea3 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea">
            <text jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/text" text="Enter User College Details" />
            <textarea jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea/variant" placeholder="Eg. Delhi University" />
          </textarea3>
          <textarea4 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea">
            <text jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/text" text="Enter User Occupation" />
            <textarea jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea/variant" placeholder="Eg. Software Developer" />
          </textarea4>
          <textarea5 jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea">
            <text jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/content/text" text="Enter Hobbies" />
            <textarea jcr:primaryType="nt:unstructured" sling:resourceType="typerefinery/components/forms/textarea/variant" placeholder="Eg. Coding" />
          </textarea5>
        </container1>
      </maincontainer>
    </rootcontainer>
  </jcr:content>
</jcr:root>
